#: src/install-upgrade-kernel:62
#, sh-format
msgid "USAGE: install-upgrade-kernel [install|upgrade] [huge|generic]"
msgstr "ΧΡΗΣΗ: install-upgrade-kernel [install|upgrade] [huge|generic]"

#: src/install-upgrade-kernel:64
#, sh-format
msgid "   OPTIONS:"
msgstr "   ΕΠΙΛΟΓΕΣ:"

#: src/install-upgrade-kernel:65
#, sh-format
msgid "   install  Install huge or generic kernel"
msgstr "   install  Εγκατάσταση huge ή generic πυρήνα"

#: src/install-upgrade-kernel:66
#, sh-format
msgid "   upgrade  Upgrade huge or generic kernel removing the old one"
msgstr "   upgrade  Αναβάθμιση huge ή generic πυρήνα απεγκαθιστώντας τον παλιό"

#: src/install-upgrade-kernel:67
#, sh-format
msgid "   huge     Download huge kernel"
msgstr "   huge     Λήψη huge πυρήνα"

#: src/install-upgrade-kernel:68
#, sh-format
msgid "   generic  Download generic kernel and also create an initrd"
msgstr "   generic  Λήψη generic πυρήνα και επίσης δημιούργησε το initrd"

#: src/install-upgrade-kernel:78
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "ΛΑΘΟΣ: Πάρα πολλές παράμετροι"

#: install-upgrade-kernel.desktop.in.h:1
msgid "install upgrade kernel"
msgstr "εγκατάσταση ενημέρωση πυρήνα"
